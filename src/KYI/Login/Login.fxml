   <children>
      <PasswordField fx:id="loginPasswordField" layoutX="548.0" layoutY="366.0" prefHeight="35.0" prefWidth="182.0" promptText="Password" />
      <TextField fx:id="loginEmailField" layoutX="548.0" layoutY="316.0" prefHeight="35.0" prefWidth="182.0" promptText="E-MAIL" />
      <Button fx:id="loginButton" layoutX="618.0" layoutY="419.0" mnemonicParsing="false" onAction="#onClickLogin" text="login" />
      <Label fx:id="error" layoutX="545.0" layoutY="461.0" prefHeight="26.0" prefWidth="182.0" textFill="RED" />
   </children>
